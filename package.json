{
  "name": "sprint",
  "description": "Trading technologies",
  "version": "0.2.0",
  "author": "Sprint Trading Kenya LLC",
  "license": "BSC",
  "scripts": {
    "dev": "next dev",
    "build": "next build && babel server -d dist && npm run build-babel",
    "start": "next start && node dist/server.js",
    "start:dev": "set DEBUG=http && nodemon --exec babel-node ./server/server.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "dropTable": "babel-node ./server/models/migrations dropTables",
    "createTable": "babel-node ./server/models/migrations createTables",
    "createAdmin": "babel-node ./server/models/migrations createAdmin",
    "createClient": "babel-node ./server/models/migrations createClient",
    "createAccount": "babel-node ./server/models/migrations createAccount",
    "createAccount2": "babel-node ./server/models/migrations createAccount2",
    "seed": "npm run createClient && npm run createAccount && npm run createAccount2",
    "migrate": "npm run dropTable && npm run createTable && npm run createAdmin",
    "test": "set NODE_ENV=TEST && npm run migrate && npm run seed && nyc mocha ./server/__tests__/*.js --require @babel/register --exit",
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "keywords": [
    "Sprint",
    "finance",
    "deposit",
    "withdrawals",
    "money"
  ],
  "dependencies": {
    "@geist-ui/react": "^2.2.0",
    "@geist-ui/react-icons": "^1.0.1",
    "next": "^11.1.3",
    "prettier": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "7.12.1",
    "swagger-ui-express": "4.1.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.5",
    "typescript": "^4.2.4",
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/node": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/register": "7.6.2",
    "chai": "^4.2.0",
    "chai-http": "4.3.0",
    "coveralls": "3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "mocha": "6.2.1",
    "nodemon": "1.19.3",
    "nyc": "14.1.1"
  }
}
